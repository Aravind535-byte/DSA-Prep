class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int maxi=prices[0];
        int maximum = 0;
       for(int i=0;i<prices.size();i++){
        if(maxi>=prices[i]){
            maxi=prices[i];
        }
        maximum = max(maximum,prices[i]-maxi);
       } 
       return maximum;
    }
};
